                            <div class="col-sm-4 col-xs-12 col-md-4">
                                <div class="form-group">
                                    <label class="control-label">修改人</label>
                                    <input type="text" placeholder="修改人" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-4 col-xs-12 col-md-4">
                                <div class="form-group">
                                    <label class="control-label">作废人</label>
                                    <input type="text" placeholder="作废人" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-4 col-xs-12 col-md-4 col-lg-8">
                                <div class="form-group date">
                                    <label class="control-label">作废时间</label>
                                    <input type="text" placeholder="作废时间" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</content>